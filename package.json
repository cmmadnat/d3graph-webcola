{
  "name": "d3graph-webcola",
  "version": "1.2.1",
  "description": "D3 Graph React component on webcola",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "build": "nwb build-react-component --no-demo --copy-files",
    "build:ts": "tsc",
    "build:all": "npm-run-all copy:css-to-lib build:ts",
    "clean": "nwb clean-module && nwb clean-demo",
    "prepublishOnly": "npm run build:ts&&npm run build&&npm run build",
    "start": "nwb serve-react-demo --port 3333",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server"
  },
  "dependencies": {
    "@types/d3": "^5.7.2",
    "@types/lodash": "^4.14.149",
    "d3": "^5.15.0",
    "lodash": "^4.17.15",
    "webcola": "^3.4.0"
  },
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.0.6",
    "nwb": "0.21.x",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "ts-loader": "3.5.0",
    "typescript": "^2.9.2"
  },
